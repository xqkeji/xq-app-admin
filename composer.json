{
    "name": "xqkeji/xq-app-admin",
    "description": "一个基于新齐低代码开发框架后台管理的系统管理模块",
    "type": "library",
    "license": "SSPL-1.0",
    "autoload": {
        "psr-4": {
            "xqkeji\\app\\admin\\": "src/"
        }
    },
    "authors": [
        {
            "name": "xqkeji.cn"
        }
    ],
    "extra": {
        "post-package-install":[
            {
                "cmd":["xqkeji\\composer\\Asset","copyDirs"],
                "param":[{
                    "vendor/xqkeji/xq-app-admin/src/resource/captcha/":"resource/captcha/",
                    "vendor/xqkeji/xq-app-admin/src/resource/bootstrap/":"resource/bootstrap/",
                    "vendor/xqkeji/xq-app-admin/src/resource/bootstrap-icons/":"resource/bootstrap-icons/",
                    "vendor/xqkeji/xq-app-admin/src/resource/bootstrap-fileinput/":"resource/bootstrap-fileinput/",
                    "vendor/xqkeji/xq-app-admin/src/resource/jquery/":"resource/jquery/",
                    "vendor/xqkeji/xq-app-admin/src/resource/overlayscrollbars/":"resource/overlayscrollbars/",
                    "vendor/xqkeji/xq-app-admin/src/resource/tinymce/":"resource/tinymce/",
                    "vendor/xqkeji/xq-app-admin/src/resource/xq-admin-layout/":"resource/xq-admin-layout/",
                    "vendor/xqkeji/xq-app-admin/src/resource/xq-admin-page/":"resource/xq-admin-page/",
                    "vendor/xqkeji/xq-app-admin/src/resource/xq-treegrid/":"resource/xq-treegrid/"
                }]
            }
        ],
        "post-package-update":[
            {
                "cmd":["xqkeji\\composer\\Asset","rmDirs"],
                "param":[{
                    "resource/captcha/":"resource/captcha/",
                    "resource/bootstrap/":"resource/bootstrap/",
                    "resource/bootstrap-icons/":"resource/bootstrap-icons/",
                    "resource/bootstrap-fileinput/":"resource/bootstrap-fileinput/",
                    "resource/jquery/":"resource/jquery/",
                    "resource/overlayscrollbars/":"resource/overlayscrollbars/",
                    "resource/tinymce/":"resource/tinymce/",
                    "resource/xq-admin-layout/":"resource/xq-admin-layout/",
                    "resource/xq-admin-page/":"resource/xq-admin-page/",
                    "resource/xq-treegrid/":"resource/xq-treegrid/"
                }]
            },
            {
                "cmd":["xqkeji\\composer\\Asset","copyDirs"],
                "param":[{
                    "vendor/xqkeji/xq-app-admin/src/resource/captcha/":"resource/captcha/",
                    "vendor/xqkeji/xq-app-admin/src/resource/bootstrap/":"resource/bootstrap/",
                    "vendor/xqkeji/xq-app-admin/src/resource/bootstrap-icons/":"resource/bootstrap-icons/",
                    "vendor/xqkeji/xq-app-admin/src/resource/bootstrap-fileinput/":"resource/bootstrap-fileinput/",
                    "vendor/xqkeji/xq-app-admin/src/resource/jquery/":"resource/jquery/",
                    "vendor/xqkeji/xq-app-admin/src/resource/overlayscrollbars/":"resource/overlayscrollbars/",
                    "vendor/xqkeji/xq-app-admin/src/resource/tinymce/":"resource/tinymce/",
                    "vendor/xqkeji/xq-app-admin/src/resource/xq-admin-layout/":"resource/xq-admin-layout/",
                    "vendor/xqkeji/xq-app-admin/src/resource/xq-admin-page/":"resource/xq-admin-page/",
                    "vendor/xqkeji/xq-app-admin/src/resource/xq-treegrid/":"resource/xq-treegrid/"
                }]
            }
        ],
        "post-package-uninstall":[
            {
                "cmd":["xqkeji\\composer\\Asset","rmDirs"],
                "param":[{
                    "resource/captcha/":"resource/captcha/",
                    "resource/bootstrap/":"resource/bootstrap/",
                    "resource/bootstrap-icons/":"resource/bootstrap-icons/",
                    "resource/bootstrap-fileinput/":"resource/bootstrap-fileinput/",
                    "resource/jquery/":"resource/jquery/",
                    "resource/overlayscrollbars/":"resource/overlayscrollbars/",
                    "resource/tinymce/":"resource/tinymce/",
                    "resource/xq-admin-layout/":"resource/xq-admin-layout/",
                    "resource/xq-admin-page/":"resource/xq-admin-page/",
                    "resource/xq-treegrid/":"resource/xq-treegrid/"
                }]
            }
        ]
    },
    "require": {
        "xqkeji/composer":"^1.0",
        "xqkeji/xq-app-base": "^1.0",
        "xqkeji/xq-app-attach": "^1.0"
    }
}
